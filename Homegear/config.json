{
  "name": "Homegear",
  "version": "1.0.4",
  "slug": "rpi-homegear",
  "description": "Homegear as a Hassio add-on",
  "startup": "before",
  "boot": "auto",  
  "ingress": true,
  "ingress_port": 2001,
  "gpio": true,
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "map": ["config:rw", "share:rw"],
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "ports": {
    "2001/tcp": 2001,
    "2002/tcp": 2002,
    "2003/tcp": 2003
  },
  "environment": {
  },
  "options": {
  },
  "schema": {
  },
  "devices": [
    "/dev/bus/usb:/dev/bus/usb:rwm",
    "/dev/ttyAMA0:/dev/ttyUSB0:rwm"
  ]
}
