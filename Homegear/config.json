{
  "name": "Homegear",
  "version": "1.0.1",
  "slug": "rpi-homegear",
  "description": "Homegear as a Hassio add-on",
  "startup": "before",
  "boot": "auto",
  "gpio": true,
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "map": ["config:rw", "share:rw"],
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "ports": {
    "2001/tcp": 2001,
    "2002/tcp": 2002,
    "2003/tcp": 2003
  },
  "environment": {
  },
  "options": {
  },
  "schema": {
  },
  "devices": [
    "/dev/bus/usb:/dev/bus/usb:rwm",
    "/dev/serial/by-id/usb-FTDI_FT232R_USB_UART_A95O5AFY-if00-port0:/dev/ttyUSB0:rwm"
  ]
}
